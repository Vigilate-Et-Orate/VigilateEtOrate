{
  "expo": {
    "name": "Vigilate & Orate",
    "description": "Get notifications for prayer",
    "slug": "vigilate-et-orate",
    "version": "2.0.0",
    "sdkVersion": "40.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "privacy": "unlisted",
    "backgroundColor": "#EBEBEB",
    "primaryColor": "#35415A",
    "scheme": "vigilateetorate",
    "githubUrl": "https://github.com/awalshy/VigilateEtOrate",
    "updates": {
      "fallbackToCacheTimeout": 300
    },
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#35415A"
    },
    "notification": {
      "icon": "./assets/icon-notif.png",
      "color": "#35415A"
    },
    "assetBundlePatterns": [
      "./assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.vigilateetorate.app",
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/icon-medium.png",
      "backgroundColor": "#35415A",
      "useNextNotificationsApi": true,
      "versionCode": 9,
      "splash": {
        "mdpi": "./assets/splashScreen.png",
        "hdpi": "./assets/splashScreen.png",
        "xhdpi": "./assets/splashScreen.png",
        "xxhdpi": "./assets/splashScreen.png",
        "xxxhdpi": "./assets/splashScreen.png",
        "resizeMode": "cover",
        "backgroundColor": "#35415A"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-adaptive.png",
        "backgroundImage": "./assets/icon.png",
        "backgroundColor": "#35415A"
      }
    }
  }
}
